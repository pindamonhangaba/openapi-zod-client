{
  "name": "@zod-api/openapi-zod-client",
  "version": "2.0.7",
  "license": "ISC",
  "exports": {
    ".": "./lib/src/index.ts",
    "./cli": "./cli.ts"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "cli": "deno run -A cli.ts",
    "openapi-zod-client": "deno run -A cli.ts",
    "test": "deno test -A --parallel",
    "test:watch": "deno test -A --watch"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/assert/equals": "jsr:@std/assert@1/equals",
    "@std/fs": "jsr:@std/fs@^1.0.8",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/testing/bdd": "jsr:@std/testing@^1.0.9/bdd",
    "@std/testing/snapshot": "jsr:@std/testing@^1.0.9/snapshot",
    "@std/expect": "jsr:@std/expect@^1.0.9",
    "@apidevtools/swagger-parser": "npm:@apidevtools/swagger-parser@^10.1.0",
    "fast-glob": "npm:fast-glob@^3.3.2",
    "@zodios/core": "npm:@zodios/core@10.9.6",
    "cac": "npm:cac@^6.7.14",
    "handlebars": "npm:handlebars@^4.7.7",
    "openapi-types": "npm:openapi-types@^12.0.2",
    "openapi3-ts": "npm:openapi3-ts@3.1.0",
    "pastable": "npm:pastable@^2.2.1",
    "pastable/server": "npm:pastable@^2.2.1/server",
    "prettier": "npm:prettier@^2.8.8",
    "prettier/parser-typescript": "npm:prettier@^2.8.8/parser-typescript",
    "ts-pattern": "npm:ts-pattern@^5.0.1",
    "tanu": "npm:tanu@^0.1.13",
    "tanu/dist/type": "npm:tanu@^0.1.13/dist/type",
    "whence": "npm:whence@^2.0.0",
    "zod": "npm:zod@^3.24.1"
  },
  "publish": {
    "include": [
      "cli.ts",
      "lib/src/**/*.ts",
      "lib/src/templates/**/*.hbs",
      "README.md",
      "deno.json"
    ],
    "exclude": [
      "**/*.test.ts",
      "lib/tests/**"
    ]
  }
}
