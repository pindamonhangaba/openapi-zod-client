          .optional(),
      },
      {
        name: "baz",
        type: "Query",
        schema: z
          .union([z.literal(1.3), z.literal(34.1), z.literal(-57.89)])
          .describe(
            \`baz
multiline
description\`
          )
          .optional(),
      },
      {
        name: "qux",
        type: "Query",
        schema: z.string().optional(),
      },
    ],
    response: z.void(),
  },
